{
  "expo": {
    "name": "Colombinizer",
    "slug": "colombinizer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/colombinizer-icon.png",
    "scheme": "colombinizer",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nafannn.colombinizer",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/colombinizer-icon.png",
        "backgroundColor": "#001D47"
      },
      "package": "com.nafannn.colombinizer",
      "edgeToEdgeEnabled": true
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/colombinizer-icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/colombinizer-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#001D47"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "bea83cb5-52dd-4717-af4f-b353c38f2ec4"
      }
    }
  }
}